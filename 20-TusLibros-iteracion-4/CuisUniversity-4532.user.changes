	store := Store withClientAuthenticator: (ClientAuthenticator withClient: 'admin' password: 'admin')
	clock: GregorianDateTime 
	priceList: (Dictionary with: 'validBook'->10)
	merchantProcessor: MerchantProcessorSimulator new.
	
